# Commit
function commit() {
    message="$1"

    if [ "$message" = "" ]; then
        message="wip"
    fi

    git add .
    eval "git commit -a -m '${message}'"
}

# Push
function push() {
    branch="$1"

    if [ "$branch" = "" ]; then
        branch="master"
    fi

    eval "git push origin ${branch}" 
}
