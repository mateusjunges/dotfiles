
# Alias definitions.
# You may want to put all your additions into a separete file like
# ~/.aliases, instead of adding them directly.

# Apache 2
if [ -f ~/.apache2 ]; then
    . ~/.apache2
fi

# PHP Artisan:
if [ -f ~/.artisan ]; then
    . ~/.artisan
fi 

# Composer:
if [ -f ~/.composer ]; then
    . ~/.composer
fi

# Git:
if [ -f ~/.git ]; then
    . ~/.git
fi

# mysql:
if [ -f ~/.mysql ]; then
    . ~/.mysql
fi

#phpunit
if [ -f ~/.phpunit ]; then
    . ~/.phpunit
fi

# PostgreSQL:
if [ -f ~/.postgres ]; then
    . ~/.postgres
fi

# Redis:
if [ -f ~/.redis ]; then
    . ~/.redis
fi

# VPN UEPG:
if [ -f ~/.redis ]; then
    . ~/.redis
fi

